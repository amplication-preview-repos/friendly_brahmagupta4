datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  authorId  Int?
  content   String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  published Boolean?
  title     String?
  updatedAt DateTime @updatedAt
}
