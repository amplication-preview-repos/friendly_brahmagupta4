datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Comment {
  authorName String?
  content    String?
  createdAt  DateTime  @default(now())
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  postId     Int?
  timestamp  DateTime?
  updatedAt  DateTime  @updatedAt
}
